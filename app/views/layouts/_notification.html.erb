
<li id="notification-<%= notification.id %>" class="dropdown-item d-flex justify-content-between align-items-center">
  <%= link_to notification.to_notification.message, notification.to_notification.url %>
  <%= button_to "Delete", destroy_notification_path(notification), method: :delete, class: "btn btn-sm btn-danger", form: { data: { turbo_confirm: "削除しますか？" } }%>
</li>
